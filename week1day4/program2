let amount=500
let discount=0
let finalprice=0
if(amount>=5000){
    discount=amount*0.20
    
}
else if(amount>=3000){
    discount=amount*0.10
}
else {
    discount=amount*0
    console.log("no discount")
}

console.log("amount is :"+amount)
console.log("your discount is:"+discount)
finalprice=amount-discount
console.log("your final price is :"+finalprice)
